<?xml version="1.0"?>
<ruleset name="PHP_CodeSniffer">
    <description>PHPCS configuration file.</description>
    <file>.</file>

    <rule ref="PSR12" />

    <rule ref="Generic.Files.LineLength.TooLong">
        <severity>0</severity>
    </rule>
    <rule ref="PSR1.Files.SideEffects.FoundWithSymbols">
        <severity>0</severity>
    </rule>
</ruleset>
