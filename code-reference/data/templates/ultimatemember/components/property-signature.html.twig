<span className="phpdocumentor-signature__visibility">{{ node.visibility }}</span>
{% if node.static %}<span className="phpdocumentor-signature__static">static</span>{% endif %}
<span className="phpdocumentor-signature__type">{{ not node.type ? "mixed" : node.type|route('url')|join('|')|raw }}</span>
<span className="phpdocumentor-signature__name">`${{ node.name }}`</span>
{% if node.default is not null %} = <span className="phpdocumentor-signature__default-value">{{ node.default is not null ? node.default : '""' }}</span>{% endif %}

