{% if namespaces|default([]) is not empty %}

### Namespaces
    
<dl className="phpdocumentor-table-of-contents">
    {% for namespace in namespaces %}
        <dt className="phpdocumentor-table-of-contents__entry -namespace">{{ namespace|route('url') }}</dt>
    {% endfor %}
</dl>
{% endif %}

{% if packages|default([]) is not empty %}
### Packages
  

<dl className="phpdocumentor-table-of-contents">
    {% for package in packages %}
        <dt className="phpdocumentor-table-of-contents__entry -package">{{ package|route('url') }}</dt>
    {% endfor %}
</dl>
{% endif %}

{% if node.interfaces is not empty or node.classes is not empty %}
### Interfaces, Classes and Traits
   

<dl className="phpdocumentor-table-of-contents">
    {% for interface in node.interfaces %}
        <dt className="phpdocumentor-table-of-contents__entry -interface">{{ interface|route('url') }}</dt>
        <dd>{{ interface.summary }}</dd>
    {% endfor %}

    {% for class in node.classes %}
        <dt className="phpdocumentor-table-of-contents__entry -class">{{ class|route('url') }}</dt>
        <dd>{{ class.summary }}</dd>
    {% endfor %}

    {% for trait in node.traits %}
        <dt className="phpdocumentor-table-of-contents__entry -trait">{{ trait|route('url') }}</dt>
        <dd>{{ trait.summary }}</dd>
    {% endfor %}
</dl>
{% endif %}

{% set constants = constants(node) %}
{% set properties = properties(node) %}
{% set methods = methods(node) %}


