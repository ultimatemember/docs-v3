{% if (method.response.type and method.response.type != 'void') or method.response.description %}
<div className="text-bold">Return values</div>
<span className="phpdocumentor-signature__response_type">{{ method.response.type|route('url')|join('|')|raw }}</span>{% if method.response.description %} &mdash; <span className="phpdocumentor-return-value__description">{{ method.response.description|striptags }}</span>{% endif %}
{% endif %}
<div className="py-4"></div>
