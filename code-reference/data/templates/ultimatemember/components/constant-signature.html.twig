<code className="phpdocumentor-signature phpdocumentor-code {% if node.deprecated %}-deprecated{% endif %}">
    <span className="phpdocumentor-signature__visibility">{{ node.visibility }}</span>
    `{{ not node.type ? "mixed" : node.type|route('url')|join('|')|raw }}`
    <span className="phpdocumentor-signature__name">{{ node.name }}</span>
    = <span className="phpdocumentor-signature__default-value">{{ node.value ?: '""' }}</span>
</code>
